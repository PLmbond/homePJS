<!doctype html>
<html>

<head>
  <meta charset="utf-8" />
  <meta http-equiv="x-ua-compatible" content="ie=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <title>Request Profound.js Trial</title>
</head>

<body>
  <h1>Request Trial</h1>
  <div>
    <form method="post" action="/key/trial">
      <div><label for="first">First name:</label><input type="text" id="first" name="first" value="<%= (typeof first != "undefined") ? first : "" %>" required autofocus /></div>
      <div><label for="last">Last name:</label><input type="text" id="last" name="last" value="<%= (typeof last != "undefined") ? last : "" %>" required /></div>
      <div><label for="company">Company:</label><input type="text" id="company" name="company" value="<%= (typeof company != "undefined") ? company : "" %>" required /></div>
      <div><label for="email">Email address:</label><input type="email" id="email" name="email" value="<%= (typeof email != "undefined") ? email : "" %>" required /></div>
      <div><label for="phone">Telephone number:</label><input type="tel" id="phone" name="phone" value="<%= (typeof phone != "undefined") ? phone : "" %>" required /></div>
      <div><input type="submit" value="Submit" /></div>
    </form>
  </div>
  <% if (typeof error != "undefined") { %>
    <div><span><%= error %></span></div>
  <% } %>
</body>

</html>